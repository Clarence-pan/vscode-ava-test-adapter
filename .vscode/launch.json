{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Program",
      "program": "${file}",
      "request": "launch",
      "skipFiles": ["<node_internals>/**"],
      "type": "pwa-node"
    },
    {
      "type": "extensionHost",
      "request": "launch",
      "name": "Example adapter",
      "runtimeExecutable": "${execPath}",
      "args": ["--extensionDevelopmentPath=${workspaceFolder}"],
      "outFiles": ["${workspaceFolder}/out"]
    }
  ]
}
